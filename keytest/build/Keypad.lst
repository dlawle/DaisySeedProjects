 113:Keypad.cpp    **** // Manage the list without rearranging the keys. Returns true if any keys on the list changed state
 114:Keypad.cpp    **** bool Keypad::updateList()
 115:Keypad.cpp    **** {
 116:Keypad.cpp    **** 	bool anyActivity = false;
 117:Keypad.cpp    **** 	// Delete any IDLE keys
 118:Keypad.cpp    **** 	for (unsigned char i = 0; i < LIST_MAX; i++)
 119:Keypad.cpp    **** 	{
 120:Keypad.cpp    **** 		if (key[i].kstate == KeyState::IDLE)
 121:Keypad.cpp    **** 		{
 122:Keypad.cpp    **** 			key[i].kchar = NO_KEY;
 123:Keypad.cpp    **** 			key[i].kcode = -1;
 124:Keypad.cpp    **** 			key[i].stateChanged = false;
 125:Keypad.cpp    **** 		}
 126:Keypad.cpp    **** 	}
 127:Keypad.cpp    **** 	// Add new keys to empty slots in the key list.
 128:Keypad.cpp    **** 	for (unsigned char r = 0; r < sizeKpd.rows; r++)
 129:Keypad.cpp    **** 	{
 130:Keypad.cpp    **** 		for (unsigned char c = 0; c < sizeKpd.columns; c++)
 131:Keypad.cpp    **** 		{
 132:Keypad.cpp    **** 			bool button = bitRead(bitMap[r], c);
 133:Keypad.cpp    **** 			char keyChar = keymap[r * sizeKpd.columns + c];
 134:Keypad.cpp    **** 			int keyCode = r * sizeKpd.columns + c;
 135:Keypad.cpp    **** 			int idx = findInList(keyCode);
 136:Keypad.cpp    **** 			// Key is already on the list so set its next state.
 137:Keypad.cpp    **** 			if (idx > -1)
 138:Keypad.cpp    **** 				nextKeyState(idx, button);
 139:Keypad.cpp    **** 			// Key is NOT on the list so add it.
 140:Keypad.cpp    **** 			if ((idx == -1) && button)
 141:Keypad.cpp    **** 			{
 142:Keypad.cpp    **** 				for (unsigned char i = 0; i < LIST_MAX; i++)
 143:Keypad.cpp    **** 				{
 144:Keypad.cpp    **** 					if (key[i].kchar == NO_KEY)
 145:Keypad.cpp    **** 					{		// Find an empty slot or don't add key to list.
 146:Keypad.cpp    **** 						key[i].kchar = keyChar;
 147:Keypad.cpp    **** 						key[i].kcode = keyCode;
 148:Keypad.cpp    **** 						key[i].kstate = KeyState::IDLE;	// Keys NOT on the list have an initial state of IDLE.
 149:Keypad.cpp    **** 						nextKeyState(i, button);
 150:Keypad.cpp    **** 						break;	// Don't fill all the empty slots with the same key.
 151:Keypad.cpp    **** 					}
 152:Keypad.cpp    **** 				}
 153:Keypad.cpp    **** 			}
 154:Keypad.cpp    **** 		}
 155:Keypad.cpp    **** 	}
